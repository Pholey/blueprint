{
  "name": "@blueprintjs/site-landing",
  "version": "1.5.0",
  "description": "Blueprint landing page",
  "private": true,
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build:copy": "mkdir -p dist/assets && cp ./src/index.html dist/. && cp -r ./resources/img dist/assets/.",
    "clean": "rm -rf dist/*",
    "dev": "webpack-dev-server --config ./webpack.config.js",
    "dist": "NODE_ENV=production npm-run-all build dist:copy",
    "dist:copy": "cp -r dist/* ../../docs/.",
    "lint": "npm-run-all -p lint:stylelint lint:tslint",
    "lint:stylelint": "stylelint -s scss 'src/**/*.scss'",
    "lint:tslint": "tslint '{src,test}/**/*.{ts,tsx}' -c ../../tslint.json",
    "lint-fix": "yarn lint:tslint --fix",
    "optimize:img-svgs": "svgo --disable=collapseGroups --pretty -f resources/img",
    "optimize:inline-svgs": "svgo --disable=collapseGroups --pretty -f resources/inline && node scripts/dedupe-svg-ids.js"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.25.1",
    "@blueprintjs/webpack-build-scripts": "^0.1.0",
    "classnames": "^2.2.5",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.1",
    "react-dom": "^15.6.1",
    "svgo": "^0.7.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:palantir/blueprint.git"
  },
  "keywords": [
    "blueprint",
    "site-landing",
    "open source"
  ],
  "author": "Palantir Technologies",
  "license": "Apache-2.0"
}
